extends ../jade/layouts/layout
prepend page
  - title= 'Artists | thesongsays'
block contents
  -
    var meta = {
      title: 'Artists Page'
    }
  h1 Artists Page
  a(href="/index.html")
  p.
    This is in a p tag #{meta.title}
  ul
    each artist in artistData
      li
        a(href='/artists/#{artist.slug}') #{artist.name}
  
